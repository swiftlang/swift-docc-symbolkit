    {
      "content" : [
        {
          "anchor" : "overview",
          "level" : 2,
          "text" : "Overview",
          "type" : "heading"
        },
        {
          "inlineContent" : [
            {
              "text" : "A line list shouldn’t contain any documentation comment markers or newlines.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "inlineContent" : [
            {
              "text" : "For example, for the following C++-style documentation comment:",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "\/\/\/ First line",
            "\/\/\/ Second line",
            "void foo() {}"
          ],
          "syntax" : "c++",
          "type" : "codeListing"
        },
        {
          "inlineContent" : [
            {
              "text" : "The line list would be represented by the following LineList:",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "{",
            " \"lines\": [",
            "   {",
            "     \"text\": \"First line\",",
            "     \"range\": {",
            "       \"start\": {",
            "         \"line\": 0,",
            "         \"character\": 4",
            "       },",
            "       \"end\": {",
            "         \"line\": 0,",
            "         \"character\": 14",
            "       }",
            "     }",
            "   },",
            "   {",
            "     \"text\": \"Second line\",",
            "     \"range\": {",
            "       \"start\": {",
            "         \"line\": 1,",
            "         \"character\": 4",
            "       },",
            "       \"end\": {",
            "         \"line\": 1,",
            "         \"character\": 15",
            "       }",
            "     }",
            "   }",
            " ]",
            "}"
          ],
          "syntax" : "json",
          "type" : "codeListing"
        },
        {
          "inlineContent" : [
            {
              "text" : "The same breakdown should occur for block-style documentation comments, as in:",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "\/**",
            "* First line",
            "* Second line",
            "*\/",
            "void foo() {}"
          ],
          "syntax" : "c++",
          "type" : "codeListing"
        },
        {
          "inlineContent" : [
            {
              "text" : "or:",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "\/**",
            "First line",
            "Second line",
            "*\/",
            "void foo() {}"
          ],
          "syntax" : "c++",
          "type" : "codeListing"
        },
        {
          "inlineContent" : [
            {
              "text" : "It is the responsibility of the tool generating the symbol graph to measure and trim indentation appropriately to project a logical block of text. In all of the above cases, logically, the text is:",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "First line",
            "Second line"
          ],
          "syntax" : null,
          "type" : "codeListing"
        },
        {
          "inlineContent" : [
            {
              "text" : "That is the text content that would be parsed as markup.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "inlineContent" : [
            {
              "text" : "Line lists were chosen as the representation of documentation markup because each language may have different syntactic forms for documentation comments. This form works for both sequences of single-line comments or multi-line comment blocks.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        }
      ],
      "kind" : "content"
    }
